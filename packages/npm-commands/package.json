{
  "name": "npm-commands-smallela",
  "version": "0.0.4",
  "description": "Reusable commands for NPM",
  "author": "Sridhar Mallela",
  "license": "MIT",
  "homepage": "https://sridharmallela.github.io/smallela-workspace/npm-commands/",
  "type": "commonjs",
  "main": "./dist/index.js",
