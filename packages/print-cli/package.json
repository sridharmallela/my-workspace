{
  "name": "print-cli",
  "version": "2.1.8",
  "description": "print command line interface",
  "author": "Sridhar Mallela",
  "license": "MIT",
  "homepage": "https://sridharmallela.github.io/smallela-workspace/print-cli/",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./printer": {
      "types": "./dist/lib/printer.d.ts",
      "import": "./dist/lib/printer.js",
      "default": "./dist/lib/printer.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sridharmallela/smallela-workspace.git",
    "directory": "packages/print-cli"
  },
  "bugs": {
    "url": "https://github.com/sridharmallela/smallela-workspace/issues"
  },
  "config": {
    "unsafe-perm": true
  },
  "bin": {
    "print-cli": "./dist/index.js"
  },
  "keywords": [
    "print",
    "print cli",
    "print-cli",
    "figlet",
    "crazy print",
    "smallela",
    "sridhar",
    "mallela"
  ],
  "peerDependencies": {
    "tslib": ">=2"
  },
  "dependencies": {
    "commander": ">=13.1",
    "colors": ">=1.4",
    "figlet": ">=1.8"
  }
}
