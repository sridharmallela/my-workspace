{
  "name": "jest-preset-smallela",
  "version": "1.2.8",
  "description": "jest preset for my projects",
  "author": "Sridhar Mallela",
  "license": "MIT",
  "homepage": "https://sridharmallela.github.io/smallela-workspace/jest-preset/",
  "type": "commonjs",
  "main": "./jest-preset.cjs",
  "module": "./jest-preset.cjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./jest-preset.cjs",
      "default": "./jest-preset.cjs"
    },
    "./config": {
      "types": "./dist/lib/preset-rules.d.ts",
      "import": "./dist/lib/preset-rules.js",
      "default": "./dist/lib/preset-rules.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sridharmallela/smallela-workspace.git",
    "directory": "packages/jest-preset"
  },
  "bugs": {
    "url": "https://github.com/sridharmallela/smallela-workspace/issues"
  },
  "config": {
    "unsafe-perm": true
  },
  "keywords": [
    "jest",
    "jest config",
    "jest-config",
    "preset",
    "jest preset",
    "jest-preset",
    "smallela",
    "sridhar",
    "mallela"
  ],
  "peerDependencies": {
    "tslib": ">=2"
  },
  "dependencies": {
    "is-ci": ">=4.1"
  }
}
