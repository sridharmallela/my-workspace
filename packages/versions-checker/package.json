{
  "name": "versions-checker",
  "version": "2.1.10",
  "description": "check installed versions of node, npm, yarn, and git",
  "author": "Sridhar Mallela",
  "license": "MIT",
  "homepage": "https://sridharmallela.github.io/ngs-workspace/versions-checker/",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./checker": {
      "types": "./dist/lib/checker.d.ts",
      "import": "./dist/lib/checker.js",
      "default": "./dist/lib/checker.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sridharmallela/ngs-workspace.git",
    "directory": "packages/versions-checker"
  },
  "bugs": {
    "url": "https://github.com/sridharmallela/ngs-workspace/issues"
  },
  "config": {
    "unsafe-perm": true
  },
  "bin": {
    "versions-checker": "./dist/index.js"
  },
  "keywords": [
    "versions-checker",
    "versions",
    "checker",
    "npm version",
    "node version",
    "yarn version",
    "eslint version"
  ],
  "peerDependencies": {
    "tslib": ">=2"
  },
  "dependencies": {
    "commander": ">=13.1",
    "semver": ">=7.6"
  }
}
