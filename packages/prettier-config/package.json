{
  "name": "prettier-config-ngs",
  "version": "1.0.3",
  "description": "prettier configs for my projects",
  "author": "Sridhar Mallela",
  "license": "MIT",
  "homepage": "https://sridharmallela.github.io/ngs-workspace/prettier-config",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
