{
  "name": "eslint-config-ngs",
  "version": "1.0.2",
  "description": "Configuration for ESLint",
  "author": "Sridhar Mallela",
  "license": "MIT",
  "homepage": "https://sridharmallela.github.io/ngs-workspace/eslint-config",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./recommended": {
      "types": "./dist/recommended.d.ts",
      "import": "./dist/lib/recommended.js",
      "default": "./dist/lib/recommended.js"
    },
    "./recommended-jest": {
      "types": "./dist/recommended-jest.d.ts",
      "import": "./dist/lib/recommended-jest.js",
      "default": "./dist/lib/recommended-jest.js"
    },
    "./recommended-js": {
      "types": "./dist/recommended-js.d.ts",
      "import": "./dist/lib/recommended-js.js",
      "default": "./dist/lib/recommended-js.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sridharmallela/ngs-workspace.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/sridharmallela/ngs-workspace/issues"
  },
  "config": {
    "unsafe-perm": true
  },
  "keywords": [
    "eslint",
    "eslint config",
    "eslint-config"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=7",
    "@typescript-eslint/parser": ">=7",
    "eslint": ">=7",
    "eslint-plugin-import": ">=2.31",
    "eslint-plugin-jest": ">=27",
    "eslint-plugin-jest-formatting": ">=3"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jest-formatting": {
      "optional": true
    }
  }
}
